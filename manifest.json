{
    "manifest_version": 2,
    "name": "moveNow",
    "version": "0.1",
    "description": "This forces you to workout!",
    "permissions": [
      "tabs", 
      "background",
      "alarms"
    ],
    "browser_action": { },
    "background": {
        "scripts": [
          "background.js"
        ],
        "persistent": false
    },
    "web_accessible_resources": [
      "scripts/script.js",
      "p5.dom.min.js", 
      "p5.min.js",
      "ml5.min.js",
      "https://storage.googleapis.com/tm-model/7SX5ZKcp/model.json"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://storage.googleapis.com/tm-model/7SX5ZKcp/model.json; object-src 'self'"
}
